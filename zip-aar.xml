<?xml version='1.0' encoding='UTF-8'?>
<assembly>
    <id>zip-aar</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
 <fileSets>
        <fileSet>
            <directory>${basedir}/target</directory>
            <outputDirectory>/</outputDirectory>
            <includes>
                <include>**/*.aar</include>
             </includes>
        </fileSet>

    </fileSets>

</assembly>
